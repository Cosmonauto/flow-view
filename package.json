{
  "name": "flow-view",
  "description": "is a visual editor for Dataflow programming",
  "version": "4.0.0",
  "homepage": "http://g14n.info/flow-view",
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "main": "flow-view.js",
  "style": "flow-view.css",
  "scripts": {
    "build": "run-p browserify docs:_data generate_schema postcss",
    "browserify": "cross-env NODE_ENV=production browserify ${npm_package_main} -t babelify -g [ envify --NODE_ENV production ] -g uglifyify | terser --compress --mangle > dist/${npm_package_name}.min.js",
    "docs:_data": "run-p docs:_data:*",
    "docs:_data:package.json": "cp package.json docs/_data/",
    "docs:_data:tags.json": "wget https://api.github.com/repos/fibo/${npm_package_name}/tags -O docs/_data/tags.json",
    "generate_schema": "markdown2code --lang yaml README.md | cson2json > docs/schema.json",
    "homepage": "echo \"---\ntitle: $npm_package_name\nflow: /empty.json\n---\" > docs/index.md; cat README.md >> docs/index.md",
    "lint": "standa",
    "preversion": "npm run build",
    "postcss": "postcss ${npm_package_name}.css --output dist/${npm_package_main}.min.css",
    "postversion": "git push origin v${npm_package_version}; npm publish",
    "postpublish": "npm run docs:_data; git commit -am ':arrow_up: updated version'; git push origin master",
    "test": "node test/jsonSchema.js",
    "start": "http-server -o"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/flow-view.git"
  },
  "keywords": [
    "dataflow",
    "visual",
    "svg",
    "editor",
    "vanilla"
  ],
  "bugs": {
    "url": "https://github.com/fibo/flow-view/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "browserslist": "^4.8.3",
    "core-js": "^3.6.2",
    "cross-env": "^6.0.3",
    "cson": "^6.9.0",
    "cssnano": "^4.1.10",
    "envify": "^4.1.0",
    "http-server": "^0.12.1",
    "is-my-json-valid": "^2.19.0",
    "markdown2code": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "pre-commit": "^1.2.2",
    "standa": "^14.0.0",
    "terser": "^4.6.2",
    "uglifyify": "^5.0.2"
  }
}
